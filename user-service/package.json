{
  "name": "user-service",
  "version": "1.0.0",
  "description": "User authentication and profile management service",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:generate": "drizzle-kit generate --config=drizzle.config.js",
    "db:push": "drizzle-kit push --config=drizzle.config.js",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.js",
    "db:studio": "drizzle-kit studio --config=drizzle.config.js"
  },
  "keywords": [
    "user",
    "authentication",
    "jwt",
    "microservice"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.44.6",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "drizzle-kit": "^0.18.1",
    "nodemon": "^3.0.1"
  }
}
